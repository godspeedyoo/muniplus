


<div class="container">
  <div class="sharktank eight offset-by-two columns">
    <h3><%= @title %></h3>
    <table class="hidden">
    <tr>
      <th>Description</th>
      <th>Value</th>
    </tr>
    <% @stop.each_pair do |k, v| %>
    <tr>
      <td><%= k %></td>
      <td id="<%= k %>"><%= v %></td>
    </tr>
    <% end %>
    </table>

    <br>
    <div class="clock"></div>
    <br>
    <div class="clock_2"></div>

    <div id="map-canvas">
    </div>
  </div>
</div>


